{% extends 'base.html.twig' %}

{% block title %}Hello DemoController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>âœ… Excited Cells</h1>
    <table style="width: 100%">
    <tr>
        <th style="text-align: center">STEP(S)</th>
        <th style="text-align: center">CELLS SEQUENCE</th>
        <th style="text-align: center">RESULT</th>
    </tr>
    {% for result in results %}
    <tr>
        <td style="text-align: center"> {{ result.steps }}</td>
        <td style="text-align: center">{{ result.sequence }}</td>
        <td style="text-align: center">{{ result.result }}</td>
    </tr>
    {% endfor %}
    </table>
</div>

{% endblock %}
